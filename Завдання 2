let calculator = {
num1: 0,
num2: 0,
  sum() {
    return this.num1 + this.num2;
  },
 mul() {
    return this.num1 * this.num2;
  },
 read() {
    this.num1 = +prompt('number 1:', 0);
    this.num2 = +prompt('number 2:', 0);
  }
};
calculator.read();
alert( calculator.sum() );
alert( calculator.mul() );
